FROM Film, Proiezioni, Sale
WHERE Film.CodFilm = Proiezioni.CodFilm
AND Sale.CodSala = Proiezioni.CodSala
AND Sale.Citta  LIKE 'pisa'
AND Film.CodFilm NOT IN (SELECT Proiezioni.CodFilm
														FROM Proiezioni, Sale
														WHERE Sale.CodSala = Proiezioni.CodSala
														AND Sale.Citta NOT LIKE 'pisa')</sql><current_tab id="0"/></tab_sql></sqlb_project>
